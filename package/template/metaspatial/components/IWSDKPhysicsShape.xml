<ComponentSchema packageName="com.iwsdk.components">
  <Enum name="PhysicsShapeType">
    <EnumValue value="Sphere"/>
    <EnumValue value="Box"/>
    <EnumValue value="Cylinder"/>
    <EnumValue value="Capsules"/>
    <EnumValue value="ConvexHull"/>
    <EnumValue value="TriMesh"/>
    <EnumValue value="Auto"/>
  </Enum>
  <Component name="PhysicsShape" description="Component to define physics shape of an entity.">
    <EnumAttribute name="shape" defaultValue="PhysicsShapeType.Auto"/>
    <Vector3Attribute name="dimensions" defaultValue="0f, 0f, 0f"/>
    <FloatAttribute name="density" defaultValue="1f"/>
    <FloatAttribute name="restitution" defaultValue="0f"/>
    <FloatAttribute name="friction" defaultValue="0.5f"/>
  </Component>
</ComponentSchema>