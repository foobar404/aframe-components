    <StringAttribute name="semanticLabel" defaultValue=""/>
    <Vector3Attribute name="min" defaultValue="0f, 0f, 0f"/>
    <Vector3Attribute name="max" defaultValue="0f, 0f, 0f"/>
    <Vector3Attribute name="dimensions" defaultValue="0f, 0f, 0f"/>
  </Component>
</ComponentSchema>